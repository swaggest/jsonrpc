{
  "openapi": "3.0.3",
  "info": {
    "title": "FC App",
    "description": "FinCompare Main App.",
    "version": "v0.0.0"
  },
  "paths": {
    "/api/v1/inquiry/{inquiry_code}/offer": {
      "post": {
        "tags": [
          "Inquiry"
        ],
        "summary": "Inquiry Offer Send",
        "operationId": "v1_inquiry_fi-automation_send_offer",
        "description": "If fi-automation requests offer sends it to fc-app",
        "parameters": [
          {
            "name": "inquiry_code",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
          ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {"$ref":"#/components/schemas/InquiryOfferUpload"}
            }
          }
        },
        "responses": {
          "201": {
            "description": "successful operation"
          }
        },
        "security": [
          {
            "serviceAuth": []
          }
        ]
      }
    },
    "/api/v1/finance-request/{application_code}/fi-automation/appdata": {
      "get": {
        "tags": [
          "Application"
        ],
        "summary": "Application Data",
        "operationId": "v1_finance-request_fi-automation_appdata_list",
        "description": "If fi-automation requests application data fc-app sends it",
        "parameters": [
          {
            "name": "application_code",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Application data for fi-automation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApplicationResponse"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "detail": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "serviceAuth": []
          }
        ]
      }
    }
  },
